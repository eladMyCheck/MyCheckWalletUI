{
  "name": "MyCheckWalletUI",
  "version": "0.0.12",
  "summary": "An SDK that supplies UI for payment method managment.",
  "description": "README.md",
  "homepage": "https://mycheckapp.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "elad schiller": "eladsc@mycheck.co.il"
  },
  "source": {
    "git": "https://bitbucket.org/erez_spatz/mycheckwalletui-ios.git",
    "tag": "0.0.12"
  },
  "platforms": {
    "ios": "9.3"
  },
  "requires_arc": "true",
  "dependencies": {
    "Alamofire": [
      "~> 4.0"
    ],
    "Kingfisher": [
      "~> 3.0"
    ]
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "MyCheckWalletUI/Classes/*",
      "resource_bundles": {
        "MyCheckWalletUI": [
          "MyCheckWalletUI/Assets/*"
        ]
      }
    },
    {
      "name": "PayPal",
      "dependencies": {
        "MyCheckWalletUI/Core": [

        ],
        "Braintree/PayPal": [

        ],
        "Braintree/DataCollector": [

        ]
      },
      "source_files": "MyCheckWalletUI/Classes/paypal/**/*",
      "platforms": {
        "ios": "9.3"
      }
    },
    {
      "name": "MasterPass",
      "platforms": {
        "ios": "9.3"
      },
      "source_files": "MyCheckWalletUI/Classes/MasterPass/**/*"
    }
  ]
}
